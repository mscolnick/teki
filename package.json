{
  "name": "teki",
  "version": "1.0.0",
  "description": "Teki - The unreasonably efficient TypeScript route parser.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn lint && yarn test && yarn clean && yarn build:prod && yarn build:iife",
    "clean": "rm -rf dist cdn",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet --fix",
    "build:iife": "rollup -c rollup.iife.config.js --environment NODE_ENV:production",
    "build:prod": "rollup -c --environment NODE_ENV:production",
    "pub": "yarn prod && yarn publish",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "rollup -cw"
  },
  "types": "dist/cjs/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/philipnilsson/teki"
  },
  "author": "Philip Nilsson",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "jest": "^26.1.0",
    "rollup": "^2.18.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^6.0.4",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-jest": "^26.1.4",
    "typescript": "^3.8.3"
  }
}
